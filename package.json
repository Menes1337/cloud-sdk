{
  "name": "@shopgate/cloud-sdk",
  "version": "0.0.9",
  "description": "Shopgate's Cloud SDK",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive test",
    "cover": "nyc mocha --recursive test",
    "lint": "standard",
    "lint-staged": "lint-staged",
    "check": "nsp check"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shopgate/cloud-sdk.git"
  },
  "bin": {
    "sgcloud": "./bin/sgcloud"
  },
  "keywords": [
    "shopgate",
    "cloud",
    "sdk"
  ],
  "contributors": [
    "Metin Kul <metinkul@outlook.com> (https://github.com/daddy-cool)",
    "Michael Freund <michael.freund@shopgate.com> (https://github.com/xFragger)",
    "Daniel Huth <daniel.huth@shopgate.com> (https://github.com/Agreon)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/shopgate/cloud-sdk/issues"
  },
  "homepage": "https://github.com/shopgate/cloud-sdk",
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "commander": "^2.11.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "inquirer": "^3.3.0",
    "mkdirp": "^0.5.1",
    "request": "^2.83.0",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "lint-staged": "^4.3.0",
    "mocha": "^4.0.1",
    "mock-require": "^2.0.2",
    "mock-stdin": "^0.3.1",
    "nock": "^9.0.25",
    "nsp": "^2.8.1",
    "nyc": "^11.3.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.2",
    "socket.io": "^2.0.4",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ],
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "build",
      "test"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "report-dir": "build"
  }
}
